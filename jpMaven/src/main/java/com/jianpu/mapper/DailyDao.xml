<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jianpu.dao.DailyDao">

	
	<insert id="insertDaily" parameterType="Daily">
		insert into t_daily(username,date,detail,type) values(#{username},#{date},#{detail},#{type})
		
		
	</insert>
	
	<select id="selectDaily" resultType="Daily">
		select * from t_daily
	
	</select>
	
	
	
	
</mapper>